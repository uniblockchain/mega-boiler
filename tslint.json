{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "class-name": false,
    "no-empty": false,
    "no-consecutive-blank-lines": false,
    "no-console": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "max-line-length": [
      300
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "interface-name": [
      false
    ],
    "member-ordering": [
      false
    ],
    "curly": false
  }
}
