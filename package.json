{
  "name": "mega-boiler",
  "version": "1.0.0",
  "description": "Mega boiler for quick coding",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/jest-cli/bin/jest.js",
    "eslint": "./node_modules/eslint/bin/eslint.js --color **/*.js",
    "tslint": "./node_modules/tslint/bin/tslint --type-check --project ./tsconfig.json",
    "start": "node --use_strict --harmony --es_staging index.js",
    "tsc": "./node_modules/typescript/bin/tsc"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/mega-boiler.git"
  },
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/mega-boiler/issues"
  },
  "homepage": "https://github.com/SalvatorePreviti/mega-boiler#readme",
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/collections": "^5.0.0",
    "@types/jest": "^20.0.2",
    "@types/node": "^8.0.13",
    "chalk": "^2.0.1",
    "collections": "^5.0.6",
    "eslint": "^4.2.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-node": "^5.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "source-map-support": "^0.4.15",
    "ts-jest": "^20.0.7",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^2.4.1"
  },
  "engines": {
    "node": ">=8.1.3"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "((test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {}
}
